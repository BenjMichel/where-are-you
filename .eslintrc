{
  "extends": [
    "airbnb"
  ],
  "parser": "babel-eslint",
  "plugins": [
  ],
  "env": {
    "browser": true,
    "node": true,
    "mocha": true
  },
  "ecmaFeatures": {
    "jsx": true,
    "es6": true,
    "classes": true
  },
  "rules": {
    "no-underscore-dangle": 0,
    "no-confusing-arrow": 1,
    "max-len": [1, 180, 4],
    "arrow-body-style": [0],
    "react/jsx-no-bind": [0],
    "react/jsx-filename-extension": 0
  }
}
